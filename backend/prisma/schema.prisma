generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model User {
  id            Int       @id @default(autoincrement())
  firstName     String
  lastName      String
  company       String?
  plant         String
  email         String
  phone         String
  meetingWith   String?
  createdAt     DateTime  @default(now())
  signedOutAt   DateTime?
}